<SCENE>

  <INITIALS>
    <frustum near="0" far="0.1" />
    <translate x="1" y="1" z="-2" />
    <rotation axis="x" angle="1" />
    <rotation axis="y" angle="-3" />
    <rotation axis="z" angle="2" />
    <scale sx="1" sy="2" sz="-1" />
    <reference length="20" />
  </INITIALS>

  <ILLUMINATION>
    <ambient r="0" g="0" b="0" a="1" />
    <background r="0.6" g="0.1" b="0.2" a="0.5" />
  </ILLUMINATION>

  <LIGHTS>

    <LIGHT id="esquerda">
      <enable value="1" />
      <position x="1" y="2" z="3" w="4" />
      <ambient r="1" g="0.1" b="0.2" a="0.5" />
      <diffuse r="0.1" g="0.3" b="0.4" a="0.7" />
      <specular r="0.2" g="0.4" b="0.6" a="0.9" />
    </LIGHT>
    <LIGHT id="direita">
      <enable value="0" />
      <position x="1" y="0.1" z="0.2" w="0.5" />
      <ambient r="1" g="0.2" b="0.3" a="0.4" />
      <diffuse r="0.2" g="0.4" b="0.6" a="0.9" />
      <specular r="0.1" g="0.3" b="0.4" a="0.7" />
    </LIGHT>

  </LIGHTS>

  <TEXTURES>

    <TEXTURE id="ss">
      <file path="somePath" />
      <amplif_factor s="0.1" t="0.2" />
    </TEXTURE>

  </TEXTURES>

  <MATERIALS>

    <MATERIAL id="ss">
      <shininess value="20" />
      <specular r="0.1" g="0.2" b="0.3" a="0.4" />
      <diffuse r="0.1" g="0.3" b="0.5" a="0.7" />
      <ambient r="0.1" g="0.3" b="0.5" a="0.7" />
      <emission r="0.1" g="0.3" b="0.5" a="0.7" />
    </MATERIAL>

  </MATERIALS>

  <LEAVES>

    <LEAF id="rect" type="rectangle" args="1 2 3 4" />
    <LEAF id="cyli" type="cylinder" args="1.0 1.1 1.2 1 1" />
    <LEAF id="sphe" type="sphere" args="1.2 10 11" />
    <LEAF id="tria" type="triangle" args="1 2 3  4 5 6  7 8 9" />

  </LEAVES>

  <NODES>

    <ROOT id="rootId" />

    <NODE id="rootId">

      <MATERIAL id="ss" />

      <TEXTURE id="clear" />

      <TRANSLATION x="-1" y="-2" z="+1" />

      <DESCENDANTS>

        <DESCENDANT id="node1" />

      </DESCENDANTS>

    </NODE>

    <NODE id="node1">

      <MATERIAL id="null" />

      <TEXTURE id="null" />

      <TRANSLATION x="1" y="2" z="-1" />
      <ROTATION axis="x" angle="-60" />
      <SCALE sx="1" sy="3" sz="-1" />
      <TRANSLATION x="1" y="2" z="-1" />
      <SCALE sx="1" sy="3" sz="-1" />

      <DESCENDANTS>

        <DESCENDANT id="ss" />


      </DESCENDANTS>

    </NODE>

    <NODE id="ss">

      <MATERIAL id="ss" />

      <TEXTURE id="ss" />

      <TRANSLATION x="2" y="1" z="-2" />
      <ROTATION axis="y" angle="20" />
      <SCALE sx="1" sy="3" sz="-1" />

      <DESCENDANTS>

        <DESCENDANT id="rect" />

      </DESCENDANTS>

    </NODE>

  </NODES>

</SCENE>
